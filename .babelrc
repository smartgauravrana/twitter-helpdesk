{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ],
      "@babel/react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "root": ["./src"]
        }
      ],
      [
        "import",
        { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
      ]
    ]
  }
  